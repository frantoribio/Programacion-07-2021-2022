<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="260.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.joseluisgs.agenda.controllers.AcercaDeController">
   <children>
      <ImageView fitHeight="117.0" fitWidth="126.0" layoutX="36.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/author.png" />
         </image>
      </ImageView>
      <Label fx:id="titulo" contentDisplay="CENTER" layoutX="177.0" layoutY="20.0" prefHeight="42.0" prefWidth="286.0" text="Acerca De" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label fx:id="version" contentDisplay="CENTER" layoutX="177.0" layoutY="71.0" prefHeight="18.0" prefWidth="211.0" text="Version" textAlignment="CENTER" />
      <Label fx:id="autor" layoutX="177.0" layoutY="106.0" prefHeight="29.0" prefWidth="286.0" text="Jose Luis Gonzalez Sanchez" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ImageView fx:id="githubIcon" fitHeight="30.0" fitWidth="30.0" layoutX="186.0" layoutY="144.0" onMouseClicked="#openGitHub" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/github.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="twitterIcon" fitHeight="30.0" fitWidth="30.0" layoutX="186.0" layoutY="185.0" onMouseClicked="#openTwitter" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/twitter.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Hyperlink fx:id="githubLink" layoutX="222.0" layoutY="147.0" prefHeight="24.0" prefWidth="225.0" text="Hyperlink" />
      <Hyperlink fx:id="twitterLink" layoutX="222.0" layoutY="188.0" prefHeight="24.0" prefWidth="225.0" text="Hyperlink" />
      <Button layoutX="403.0" layoutY="215.0" mnemonicParsing="false" onAction="#aceptarAction" text="Aceptar" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
